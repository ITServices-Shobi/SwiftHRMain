                                                            <tr>
                                                                <th> Employee No</th>
                                                                <th> Employee</th>
                                                                <th> Earnings</th>
                                                                <th>Date</th>
                                                                <th> Amount</th>
                                                                <th>Status</th>
                                                                <th>EffectedDate</th>
                                                                <th>Remark</th>
                                                                <th> Action</th>
                                                            </tr>
                                                        </tfoot>
                                                    }
