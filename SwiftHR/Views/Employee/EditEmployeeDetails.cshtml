                        //alert("alert1");
                        document.getElementById("successBody").innerHTML = usrMsgData[0];
                        //alert("alert2");
                        document.getElementById("modal-success").style.display = "block";
                        //alert("alert3");
                        setTimeout(successAlertClose, 5000);
                        //alert("alert4");
