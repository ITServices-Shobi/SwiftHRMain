@model SwiftHR.Models.Employee

@{
    ViewData["Title"] = "Edit Employee Details";
}


<form asp-controller="Employee" asp-action="EditEmployeeDetails" method="post">

    <button type="submit" class="close" style="margin-right: 2%; margin-top: 1%;" title="Close Window">
        <span aria-hidden="true">&times;</span>
    </button>

    <section class="content">

        <div class="row" style="margin-top: 2%;">
            <div class="col-md-4">
                <!-- Profile Image -->
                <div class="box box-primary">
                    <div class="box-body box-profile">
                        <img class="profile-user-img img-responsive img-circle" src="../UploadImages/@Model.EmployeeProfilePhoto" alt="User profile picture">

                        <h3 class="profile-username text-center">@Model.FirstName @Model.MiddleName @Model.LastName </h3>

                        <p class="text-muted text-center" style="font-size: 20px; font-weight: 700;"> @Model.EmployeeNumber </p>

                        <ul class="list-group list-group-unbordered">
                            <li class="list-group-item">
                                <b> Contact No </b>  <span style="color:#3c8dbc; margin-left: 4%;"> <strong> @Model.ContactNumber </strong> </span>
                            </li>
                            <li class="list-group-item">
                                <b> Email </b>  <br /> <span style="color:#3c8dbc;"><strong> @Model.Email </strong>  </span>
                            </li>
                            <li class="list-group-item">
                                <b>Department</b>  <br /> <span style="color:#3c8dbc;"><strong> @Model.Department </strong>  </span>
                            </li>
                            <li class="list-group-item">
                                <b>Designation</b>  <br /> <span style="color:#3c8dbc;"> <strong> @Model.Designation </strong> </span>
                            </li>
                            <li class="list-group-item">
                                <b>Date of Joining </b>   <span style="color:#3c8dbc; margin-left: 4%;"> <strong> @Model.DateOfJoining </strong>  </span>
                            </li>
                        </ul>

                        <a href="#" class="btn btn-primary btn-block"><b>Follow</b></a>
                    </div>
                    <!-- /.box-body -->
                </div>
            </div>
            <!-- /.col -->
            <div class="col-md-8">

                <div class="nav-tabs-custom">
                    <ul class="nav nav-tabs">
                        <li class="active"><a href="#Personal" data-toggle="tab">Personal Details</a></li>
                        <li><a href="#Employment" data-toggle="tab">Employment Details</a></li>
                        <li><a href="#Org" data-toggle="tab">Organizational Settings</a></li>
                        <li><a href="#Account" data-toggle="tab">Account Details</a></li>
                    </ul>
                    <div class="tab-content">
                        <div class="active tab-pane" id="Personal">
                            <div class="box box-solid" style="box-shadow: none !important;">
                                <div class="box-body">
                                    <div class="box-group" id="accordion">
                                        <!-- we are adding the .panel class so bootstrap.js collapse plugin detects it -->
                                        <div id="divPanelBox1" class="panel box" style="border-top: none !important; background-color:#ddd6;">
                                            <div class="box-header">
                                                <h4 class="box-title">
                                                    Basic Details
                                                </h4>
                                                <div class="box-tools pull-right">
                                                    <button id="btnPToggle1" type="button" class="btn btn-box-tool" data-toggle="collapse" data-parent="#accordion" href="#collapseOne" title="Collapse" onclick="onToggle(this)">
                                                        <i id="iconPToggle1" class="fa fa-minus"></i>
                                                    </button>
                                                </div>

                                            </div>
                                            <div id="collapseOne" class="panel-collapse collapse in">
                                                <hr style="border-top: 1px solid #d2d6de;" />
                                                <div class="box-body">
                                                    <div class="row">
                                                        <div class="col-md-4">
                                                            <div class="form-group">
                                                                <label for="txtFName">First Name</label>
                                                                <input type="text" id="txtFName" name="FirstName" asp-for="@Model.FirstName" class="form-control" placeholder="First Name" onkeydown="preventNumberInput(event)" onkeyup="preventNumberInput(event)">
                                                            </div>
                                                        </div>
                                                        <div class="col-md-4">
                                                            <div class="form-group">
                                                                <label for="txtMName">Middle Name</label>
                                                                <input type="text" id="txtMName" name="MiddleName" asp-for="@Model.MiddleName" class="form-control" placeholder="Middle Name" onkeydown="preventNumberInput(event)" onkeyup="preventNumberInput(event)">
                                                            </div>
                                                        </div>
                                                        <div class="col-md-4">
                                                            <div class="form-group">
                                                                <label for="txtLName">Last Name</label>
                                                                <input type="text" id="txtLName" name="LastName" asp-for="@Model.LastName" class="form-control" placeholder="Last Name" onkeydown="preventNumberInput(event)" onkeyup="preventNumberInput(event)">
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-md-6">
                                                            <div class="form-group">
                                                                <label for="txtMobileNo">Mobile Number</label>
                                                                <input type="text" id="txtMobileNo" name="ContactNumber" asp-for="@Model.ContactNumber" class="form-control" maxlength="10" min="10" placeholder="Mobile Number" onkeypress='return restrictAlphabets(event)' onchange="ValidateMobileNo(this)">
                                                            </div>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <div class="form-group">
                                                                <label for="txtEmail">Email </label>
                                                                <input type="email" id="txtEmail" name="Email" asp-for="@Model.Email" class="form-control" placeholder="Email" onchange="ValidateEmail(this)">
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-md-6">
                                                            <div class="form-group">
                                                                <label for="dtDateOfJoining">Date of Birth </label>
                                                                <input class="form-control" name="DateOfBirth" asp-for="@Model.DateOfBirth" type="date" id="dtDateOfBirth">
                                                            </div>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <div class="form-group">
                                                                <label for="ddlGender">Gender</label>
                                                                <select class="form-control select2" id="ddlGender" name="Gender">
                                                                    <option value="Male">Male</option>
                                                                    <option value="Female">Female</option>
                                                                    <option value="Other">Other</option>
                                                                </select>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="panel box" id="divPanelBox2" style="border-top: none !important;">
                                            <div class="box-header">
                                                <h4 class="box-title">
                                                    Additional Details
                                                </h4>
                                                <div class="box-tools pull-right">
                                                    <button id="btnPToggle2" type="button" class="btn btn-box-tool" data-toggle="collapse" data-parent="#accordion" href="#collapseTwo" title="Expand" onclick="onToggle(this)">
                                                        <i id="iconPToggle2" class="fa fa-plus"></i>
                                                    </button>
                                                </div>
                                            </div>
                                            <div id="collapseTwo" class="panel-collapse collapse">
                                                <hr style="border-top: 1px solid #d2d6de;" />
                                                <div class="box-body" style="margin-top: -3%;">
                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <div class="form-group">
                                                                <select class="form-control select2" id="ddlAddressSelection" name="AddessType" style="background: none; width: 35%;" onchange="ddlAddressSelectionChange(this.value)">
                                                                    <option value="PresentAddress">Present Address</option>
                                                                    <option value="PermanentAddress">Permanent Address</option>
                                                                </select>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div id="divPresentAddress">
                                                        <div class="row">
                                                            <div class="col-md-12">
                                                                <div class="form-group">
                                                                    <label for="txtAddress">Address</label>
                                                                    <textarea class="form-control" id="txtAddress" rows="2" name="PresentAddress" asp-for="@Model.FirstName placeholder=" Address"></textarea>
                                                                </div>
                                                            </div>

                                                        </div>
                                                        <div class="row">
                                                            <div class="col-md-3">
                                                                <div class="form-group">
                                                                    <label for="ddlCountry">Country</label>
                                                                    <select class="form-control select2" id="ddlCountry" name="Country">
                                                                        <option value="India">India</option>
                                                                        <option value="USA">USA</option>
                                                                        <option value="Austrelia">Austrelia</option>
                                                                    </select>
                                                                </div>
                                                            </div>
                                                            <div class="col-md-3">
                                                                <div class="form-group">
                                                                    <label for="ddlState">State</label>
                                                                    <select class="form-control select2" id="ddlState" name="State">
                                                                        <option value="Maharashtra">Maharashtra</option>
                                                                        <option value="Punjab">Punjab</option>
                                                                        <option value="Calcutta">Calcutta</option>
                                                                    </select>
                                                                </div>
                                                            </div>
                                                            <div class="col-md-3">
                                                                <div class="form-group">
                                                                    <label for="ddlCity">City</label>
                                                                    <select class="form-control select2" id="ddlCity" name="City">
                                                                        <option value="Pune">Pune</option>
                                                                        <option value="Mumbai">Mumbai</option>
                                                                        <option value="Delhi">Delhi</option>
                                                                    </select>
                                                                </div>
                                                            </div>
                                                            <div class="col-md-3">
                                                                <div class="form-group">
                                                                    <label for="txtPinCode">Pin Code</label>
                                                                    <input type="text" id="txtPinCode" name="PinCode" class="form-control" maxlength="6" min="6" placeholder="Pin Code" onkeypress='return restrictAlphabets(event)'>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div id="divPermanentAddress" style="display:none;">
                                                        <div class="row">
                                                            <div class="col-md-12">
                                                                <div class="form-group">
                                                                    <label for="txtPermanentAddress">Address</label>
                                                                    <textarea class="form-control" id="txtPermanentAddress" rows="2" name="PermanentAddress" placeholder="Address"></textarea>
                                                                </div>
                                                            </div>

                                                        </div>
                                                        <div class="row">
                                                            <div class="col-md-3">
                                                                <div class="form-group">
                                                                    <label for="ddlPermanentCountry">Country</label>
                                                                    <select class="form-control select2" id="ddlPermanentCountry" name="Country">
                                                                        <option value="India">India</option>
                                                                        <option value="USA">USA</option>
                                                                        <option value="Australia">Australia</option>
                                                                    </select>
                                                                </div>
                                                            </div>
                                                            <div class="col-md-3">
                                                                <div class="form-group">
                                                                    <label for="ddlPermanentState">State</label>
                                                                    <select class="form-control select2" id="ddlPermanentState" name="State">
                                                                        <option value="Maharashtra">Maharashtra</option>
                                                                        <option value="Punjab">Punjab</option>
                                                                        <option value="Calcutta">Calcutta</option>
                                                                    </select>
                                                                </div>
                                                            </div>
                                                            <div class="col-md-3">
                                                                <div class="form-group">
                                                                    <label for="ddlPermanentCity">City</label>
                                                                    <select class="form-control select2" id="ddlPermanentCity" name="City">
                                                                        <option value="Pune">Pune</option>
                                                                        <option value="Mumbai">Mumbai</option>
                                                                        <option value="Delhi">Delhi</option>
                                                                    </select>
                                                                </div>
                                                            </div>
                                                            <div class="col-md-3">
                                                                <div class="form-group">
                                                                    <label for="txtPermanentPinCode">Pin Code</label>
                                                                    <input type="text" id="txtPermanentPinCode" name="PinCode" class="form-control" maxlength="6" min="6" placeholder="Pin Code" onkeypress='return restrictAlphabets(event)'>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="panel box" id="divPanelBox3" style="border-top: none !important;">
                                            <div class="box-header">
                                                <h4 class="box-title">
                                                    Family Details
                                                </h4>
                                                <div class="box-tools pull-right">
                                                    <button id="btnPToggle3" type="button" class="btn btn-box-tool" data-toggle="collapse" data-parent="#accordion" href="#collapseThree" title="Expand" onclick="onToggle(this)">
                                                        <i id="iconPToggle3" class="fa fa-plus"></i>
                                                    </button>
                                                </div>
                                            </div>
                                            <div id="collapseThree" class="panel-collapse collapse">
                                                <hr style="border-top: 1px solid #d2d6de;" />
                                                <div class="box-body">
                                                    <div class="row">
                                                        <div class="col-md-6">
                                                            <div class="form-group">
                                                                <label for="txtMobileNo">Father's Name</label>
                                                                <input type="text" id="txtFathersName" name="FathersName" asp-for="@Model.FathersName" class="form-control" placeholder="Father's Name" onkeydown="preventNumberInput(event)" onkeyup="preventNumberInput(event)">
                                                            </div>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <div class="form-group">
                                                                <label for="txtEmail">Mother's Name </label>
                                                                <input type="text" id="txtMothersName" name="MothersName" asp-for="@Model.MothersName" class="form-control" placeholder="Mother's Name" onkeydown="preventNumberInput(event)" onkeyup="preventNumberInput(event)">
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-md-6">
                                                            <div class="form-group">
                                                                <label for="txtMobileNo">Spouse Name</label>
                                                                <input type="text" id="txtSpouseName" name="SpouseName" asp-for="@Model.SpouseName" class="form-control" placeholder="Spouse Name" onkeydown="preventNumberInput(event)" onkeyup="preventNumberInput(event)">
                                                            </div>
                                                        </div>
                                                        <div class="col-md-6">

                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-md-6">
                                                            <div class="form-group">
                                                                <label for="txtMobileNo">Nominee Name</label>
                                                                <input type="text" id="txtFathersName" name="FathersName" asp-for="@Model.NomineeName" class="form-control" placeholder="Nominee Name" onkeydown="preventNumberInput(event)" onkeyup="preventNumberInput(event)">
                                                            </div>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <div class="form-group">
                                                                <label for="txtEmail"> Nominee Contact No </label>
                                                                <input type="text" id="txtNomineeContactNo" name="NomineeContactNo" class="form-control" maxlength="10" min="10" placeholder="Nominee Contact No" onkeypress='return restrictAlphabets(event)' onchange="ValidateMobileNo(this)">
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-md-6">
                                                            <div class="form-group">
                                                                <label for="txtMobileNo">Nominee DOB</label>
                                                                <input class="form-control" name="NomineeDateOfBirth" asp-for="@Model.NomineeDob" type="date" id="dtNomineeDateOfBirth">
                                                            </div>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <div class="form-group">
                                                                <label for="txtMobileNo">Relation With Nominee </label>
                                                                <input type="text" id="txtNomineeRelation" name="NomineeRelation" asp-for="@Model.NomineeRelation" class="form-control" placeholder="Relation With Nominee" onkeydown="preventNumberInput(event)" onkeyup="preventNumberInput(event)">
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="panel box" id="divPanelBox4" style="border-top: none !important;">
                                            <div class="box-header">
                                                <h4 class="box-title">
                                                    Educational Details
                                                </h4>
                                                <div class="box-tools pull-right">
                                                    <button id="btnPToggle4" type="button" class="btn btn-box-tool" data-toggle="collapse" data-parent="#accordion" href="#collapseFour" title="Expand" onclick="onToggle(this)">
                                                        <i id="iconPToggle4" class="fa fa-plus"></i>
                                                    </button>
                                                </div>
                                            </div>
                                            <div id="collapseFour" class="panel-collapse collapse">
                                                <hr style="border-top: 1px solid #d2d6de;" />
                                                <div class="box-body">
                                                    <div class="row">
                                                        <div class="col-md-6">
                                                            <div class="form-group">
                                                                <label for="txtCourseName">Course/Degree</label>
                                                                <input type="text" id="txtCourseName" name="CourseName" class="form-control" placeholder="Course/Degree Name" onkeydown="preventNumberInput(event)" onkeyup="preventNumberInput(event)">
                                                            </div>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <div class="form-group">
                                                                <label for="txtUniversityName">Board/University </label>
                                                                <input type="text" id="txtUniversityName" name="UniversityName" class="form-control" placeholder="University Name" onkeydown="preventNumberInput(event)" onkeyup="preventNumberInput(event)">
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-md-6">
                                                            <div class="form-group">
                                                                <label for="txtMobileNo">Year of Passing </label>
                                                                <input type="text" id="txtPassingYear" name="PassingYear" class="form-control" placeholder="Year of Passing" maxlength="4" onkeypress='return restrictAlphabets(event)'>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <div class="form-group">
                                                                <label for="txtMarks">Marks(%) </label>
                                                                <input type="text" id="txtMarks" name="Marks" class="form-control" placeholder="Marks" onkeypress='return restrictAlphabets(event)'>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                    </div>
                                </div>
                                <!-- /.box-body -->
                            </div>
                        </div>
                        <!-- /.tab-pane -->
                        <div class="tab-pane" id="Employment">
                            <!-- The timeline -->
                            <ul class="timeline timeline-inverse">
                                <!-- timeline time label -->
                                <li class="time-label">
                                    <span class="bg-red">
                                        10 Feb. 2014
                                    </span>
